{
  "name": "webanyone/mautic-smsfactor-bundle",
  "description": "MauticSmsFactorBundle",
  "license": "GPL-3.0",
  "type": "mautic-plugin",
  "keywords": [
    "mautic",
    "plugin",
    "integration",
    "sms"
  ],
  "extra": {
    "install-directory-name": "MauticSmsFactorBundle",
    "branch-alias": {
      "dev-main": "0.0.x-dev"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MauticPlugin\\MauticSmsFactorBundle\\": ""
    }
  },
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.4.0 <8.1",
    "mautic/core-lib": "^4.0",
    "smsfactor/smsfactor-php-sdk": "dev-patch-1 as 1.0.5",
    "php-http/message-factory": "^1.0@dev"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "*",
    "phpunit/phpunit": "^9.5",
    "phpspec/prophecy-phpunit": "^2.0",
    "jangregor/phpstan-prophecy": "dev-master",
    "phpstan/extension-installer": "^1.2",
    "phpstan/phpstan-doctrine": "^1.3",
    "phpstan/phpstan": "^1.10"
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/ogizanagi/smsfactor-php-sdk"
    }
  ],
  "config": {
    "allow-plugins": {
      "symfony/flex": true,
      "phpstan/extension-installer": true,
      "php-http/discovery": true
    }
  }
}
